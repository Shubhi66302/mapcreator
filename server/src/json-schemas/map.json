{
  "$schema": "http://json-schema.org/schema#",
  "$id": "map",
  "title": "mapcreator's internal representation of map",
  "type": "object",
  "required": ["elevators", "zones", "queueDatas", "floors"],
  "properties": {
    "elevators": {
      "type": "array",
      "items": {
        "$ref": "elevator"
      }
    },
    "zones": {
      "type": "array",
      "items": { "$ref": "zone" }
    },
    "queueDatas": {
      "type": "array",
      "items": { "$ref": "queueData" }
    },
    "floors": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "floor_id",
          "map_values",
          "chargers",
          "ppses",
          "odses",
          "dockPoints",
          "fireEmergencies"
        ],
        "properties": {
          "floor_id": { "type": "integer", "minimum": 1 },
          "map_values": {
            "type": "array",
            "items": { "$ref": "barcodeWithNumberCoordinate" }
          },
          "chargers": {
            "type": "array",
            "items": { "$ref": "charger" }
          },
          "ppses": {
            "type": "array",
            "items": { "$ref": "pps" }
          },
          "odses": {
            "type": "array",
            "items": { "$ref": "ods" }
          },
          "dockPoints": {
            "type": "array",
            "items": { "$ref": "dockPoint" }
          },
          "fireEmergencies": {
            "type": "array",
            "items": { "$ref": "fireEmergency" }
          }
        }
      }
    }
  }
}
