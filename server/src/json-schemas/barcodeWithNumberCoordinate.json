{
  "$schema": "http://json-schema.org/schema#",
  "$id": "barcodeWithNumberCoordinate",
  "title": "Barcode with number coordinate schema",
  "description": "represents barcode as tuple of numbers eg. [12, 32]",
  "allOf": [
    { "$ref": "definitions#/barcodeWithoutCoordinate" },
    {
      "properties": {
        "coordinate": { "$ref": "definitions#/coordinate" }
      },
      "required": ["coordinate"]
    }
  ]
}
